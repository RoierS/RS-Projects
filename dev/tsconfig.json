{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "commonjs",                               
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,                
    "skipLibCheck": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  // "include": [
  //   "src/**/*.ts",
  // ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}